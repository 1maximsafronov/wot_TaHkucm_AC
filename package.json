{
  "name": "wot_tankust_ac",
  "private": true,
  "description": "Проект для сборки оборудования world of tanks",
  "devDependencies": {
    "@htmlacademy/editorconfig-cli": "1.0.0",
    "autoprefixer": "10.0.2",
    "browser-sync": "2.26.13",
    "del": "6.0.0",
    "gulp": "4.0.2",
    "gulp-csso": "4.0.1",
    "gulp-htmlmin": "5.0.1",
    "gulp-plumber": "1.2.1",
    "gulp-postcss": "9.0.0",
    "gulp-posthtml": "3.0.5",
    "gulp-rename": "2.0.0",
    "gulp-sass": "4.1.0",
